{
  "headers": [
    {
      "source": "/landing/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "CDN-Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
